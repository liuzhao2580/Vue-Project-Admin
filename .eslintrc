{
  "extends": ["plugin:vue/essential", "eslint:recommended", "@vue/typescript"],
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "warn",
    "require-await": "off",
    "indent": ["warn", 2],
    "eqeqeq": "warn"
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  }
}
